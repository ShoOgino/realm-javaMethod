    private String realmString(Realm realm) {
        String string = "";
        for (Person p : realm.allObjects(Person.class)) {
            string += "name: " +  p.getFullName() + "\n";
            string += "age: " + p.getAge() + "\n";
            string += "pets: " + p.getPets().size() + "\n";
            // FIXME - print out pet info
        }
        return string;
    }

