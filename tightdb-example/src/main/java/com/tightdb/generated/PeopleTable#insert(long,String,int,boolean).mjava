    public People insert(long position, String name, int age, boolean hired) {
        try {
        	insertString(0, position, name);
        	insertLong(1, position, age);
        	insertBoolean(2, position, hired);
        	insertDone();

        	return cursor(position);
        } catch (Exception e) {
        	throw insertRowException(e);
        }


    }

