	private static void gc2() {
	      try {
	         System.gc();
	         Thread.currentThread().sleep(100);
	         System.runFinalization();
	         Thread.currentThread().sleep(100);
	         System.gc();
	         Thread.currentThread().sleep(100);
	         System.runFinalization();
	         Thread.currentThread().sleep(100);
	      } catch (Exception e) {
	         e.printStackTrace();
	      }
	}

