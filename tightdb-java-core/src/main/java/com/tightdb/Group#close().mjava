	public void close() {
		synchronized (CloseMutex.getInstance()) {
			if (nativePtr != 0) {
				if (isValid()) {
					// System.err.println("CLOSE GROUP -------------- this " +
					// this + "   native " + nativePtr);
					nativeClose(nativePtr);
				} else {
					// System.err.println("CLOSE GROUP - INVALID!!!!");
				}
				nativePtr = 0;
			}
		}
	}

