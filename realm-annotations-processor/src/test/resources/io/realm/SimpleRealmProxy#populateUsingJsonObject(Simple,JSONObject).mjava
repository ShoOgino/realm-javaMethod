    public static void populateUsingJsonObject(Simple obj, JSONObject json)
            throws JSONException {
        boolean standalone = obj.realm == null;
        if (!json.isNull("name")) {
            obj.setName((String) json.getString("name"));
        }
        if (!json.isNull("age")) {
            obj.setAge((int) json.getInt("age"));
        }
    }

