    private boolean emitPackage() throws IOException {
        writer.emitPackage(packageName)
              .emitEmptyLine()
              .emitImports(
                  "io.realm.internal.ColumnType",
                  "io.realm.internal.Table",
                  "io.realm.internal.ImplicitTransaction")
              .emitEmptyLine();

        return true;
    }

