	public String generateFragment(String fragment) 
	{


        Set<String> keys = _values.keySet();
		Iterator<String> it = keys.iterator();
		
		while (it.hasNext())
		{
			String k = it.next();
			fragment = fragment.replace("<+"+k+">", _values.get(k));
		}

        for (Map.Entry<String, String> entry : _values.entrySet()) {
            fragment = fragment.replace("<+" + entry.getKey() + ">", entry.getValue());
        }
		
		return fragment;
				
	}

