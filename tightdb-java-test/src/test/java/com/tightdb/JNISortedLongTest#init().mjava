	void init() {
		table = new Table();
		table.addColumn(ColumnType.ColumnTypeInt, "number");
		table.addColumn(ColumnType.ColumnTypeString, "name");

		table.add(1, "A");
		table.add(10, "B");
		table.add(20, "C");
		table.add(30, "B");
		table.add(40, "D");
		table.add(50, "D");
		table.add(60, "D");
		assertEquals(7, table.size());
	}

