	/**
	 * 
	 * Returns a table with the specified name. Returns null in case the table 
	 * does not exists in group.
	 * 
	 * @param tableName
	 * @return
	 */
	public TableBase getTable(String name){
		if(hasTable(name)){
			return new TableBase(nativeGetTableNativePtr(name));
		}
		return null;
	}

