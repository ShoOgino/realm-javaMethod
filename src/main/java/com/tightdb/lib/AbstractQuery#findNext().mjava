	public Cursor findNext() {
		if (currPos < Long.MAX_VALUE) {
			currPos = query.findNext(table, currPos);
			if (currPos >= 0 && currPos < table.size()) {
				return cursor(table, currPos);
			} else {
				currPos = Long.MAX_VALUE;
				return null;
			}
		} else {
			return null;
		}
	}

