	@Test (enabled = true)
	public void shouldRemoveSomeMatchingRows() {
		// Remove some
		EmployeeQuery q = employees.where().salary.lessThan(100000000);
		
		assertEquals(1, q.count(1, 2, 1));
		
		long n = q.remove(1, 2, 1);
		assertEquals(1, n);
		assertEquals(2, employees.size());
	}

