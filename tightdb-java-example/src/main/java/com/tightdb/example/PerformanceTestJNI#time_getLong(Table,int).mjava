    static long time_getLong(Table tbl, int rows) {
    	// Loop through the table
    	Timer time = new Timer();
        for (long idx = 0; idx < rows; idx++) {
            if (tbl.getLong(0, idx) == -1) {
                break;
            }
        }
        long dur = time.GetTimeInMicroSec();
        return dur;
    }

