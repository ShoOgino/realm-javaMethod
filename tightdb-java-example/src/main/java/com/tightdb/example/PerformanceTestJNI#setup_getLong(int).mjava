    static Table setup_getLong(int rows) {
        // Create table with 1 Long column and a number of rows
        Table tbl = new Table();
        tbl.addColumn(ColumnType.INTEGER, "myInt");
        tbl.addEmptyRows(rows);
        for (long idx=0; idx<rows; idx++) {
            tbl.setLong(0, idx, 2);
        }
        return tbl;
    }

