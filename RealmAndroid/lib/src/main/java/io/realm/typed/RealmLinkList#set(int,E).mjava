    @Override
    public E set(int location, E object) {
        if(object.row == null) {
            realm.add(object);
            view.set(location, object.realmAddedAtRowIndex);
            return realm.get((Class<E>)object.getClass(), object.realmAddedAtRowIndex);
        } else {
            view.set(location, object.row.getIndex());
            return object;
        }
    }

