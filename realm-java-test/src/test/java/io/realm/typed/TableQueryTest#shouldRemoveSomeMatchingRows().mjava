    @Test
    public void shouldRemoveSomeMatchingRows() {
        // Remove some
        TestEmployeeQuery q = employees.where().salary.lessThan(100000000);

        assertEquals(1, q.count(1, 2, Table.INFINITE));

        long n = q.remove(1, 2);
        assertEquals(1, n);
        assertEquals(2, employees.size());
    }

