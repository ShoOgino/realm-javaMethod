    /**
     * {@inheritDoc}
     */
    @Override
    public E set(int location, E object) {
        if (managedMode) {
            view.set(location, object.row.getIndex());
        } else {
            assertValidObjectInNonManagedMode(object);
            nonManagedList.set(location, object);
        }
        return object;
    }

