        @Override
        public void handleMessage(Message msg) {
            switch (msg.what) {
                case MSG_SUCCESS:
                    RealmResults<E> resultList = new RealmResults<E>(callerRealm, (TableOrView) msg.obj, clazz);
                    callback.onSuccess(resultList);
                    break;
                case MSG_ERROR:
                    callback.onError((Throwable) msg.obj);
                    break;
            }
        }

