    public void testShouldFailWithNullFileName() {
        setupSharedGroup();

        try {
            Realm realm = new Realm(getContext().getFilesDir(), null);
            fail("Expected IOException");
        } catch (Exception ex) {
            if (!(ex instanceof IOException)) {
                ex.printStackTrace();
                fail("Unexpected exception: " + ex.getMessage());
            }
        }
    }

