    public void t3(long count){
        t = new Table();

        t.addColumn(RealmFieldType.UNSUPPORTED_TABLE, "table");
        t.addEmptyRow();

        for (long i=0;i<count;i++){
            t.addEmptyRow();

            Table sub = t.getSubtable(0, i);
            sub.size();
            sub.close();
        }
        
        t.close();
    }

