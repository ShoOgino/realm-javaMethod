    public void t2(long count){
        t = new Table();

        t.addColumn(RealmFieldType.UNSUPPORTED_TABLE, "table");
        t.addEmptyRow();

        for (long i=0;i<count;i++){
            t.addEmptyRow();

            TableSchema schema = t.getSubtableSchema(0);
            schema.toString();
        }
        
        t.close();
    }

