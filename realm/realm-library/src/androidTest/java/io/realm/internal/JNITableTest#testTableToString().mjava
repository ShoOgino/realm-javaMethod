    public void testTableToString() {
        Table t = new Table();

        t.addColumn(STRING, "stringCol");
        t.addColumn(INTEGER, "intCol");
        t.addColumn(BOOLEAN, "boolCol");

        t.add("s1", 1, true);
        t.add("s2", 2, false);

        String expected =
"    stringCol  intCol  boolCol\n" +
"0:  s1              1     true\n" +
"1:  s2              2    false\n" ;

        assertEquals(expected, t.toString());
    }

