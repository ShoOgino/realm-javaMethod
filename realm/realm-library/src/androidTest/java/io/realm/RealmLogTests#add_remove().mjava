    @Test
    public void add_remove() {
        TestHelper.TestLogger testLogger = new TestHelper.TestLogger();
        RealmLog.add(testLogger);
        RealmLog.fatal("TEST");
        assertEquals("TEST", testLogger.message);
        RealmLog.remove(testLogger);
        RealmLog.fatal("TEST_AGAIN");
        assertEquals("TEST", testLogger.message);
    }

