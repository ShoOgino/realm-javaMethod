    @Override
    public boolean equals(Object o) {
        if (!(o instanceof CustomMethods)) {
            return reverseEquals;
        }
        CustomMethods other = (CustomMethods) o;
        if (isValid() == other.isValid() && other.name.equals(name)) {
            return !reverseEquals;
        } else {
            return reverseEquals;
        }
    }

