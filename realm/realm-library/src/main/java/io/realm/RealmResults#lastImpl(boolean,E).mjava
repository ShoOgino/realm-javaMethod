    private E lastImpl(boolean shouldThrow, E defaultValue) {
        Row row = collection.lastUncheckedRow();

        if (row != null) {
            return realm.get(classSpec, row);
        } else {
            if (shouldThrow) {
                throw new IndexOutOfBoundsException("No results were found.");
            } else {
                return defaultValue;
            }
        }
    }

