    @Override
    public String toString() {
        StringBuilder sb = new StringBuilder(getErrorCode().toString());
        if (errorMessage != null) {
            sb.append('\n');
            sb.append(errorMessage);
        }
        if (exception != null) {
            sb.append('\n');
            sb.append(Util.getStackTrace(exception));
        }
        return sb.toString();
    }

