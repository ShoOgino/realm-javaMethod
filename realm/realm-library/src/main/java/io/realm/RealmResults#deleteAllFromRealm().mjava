    /**
     * {@inheritDoc}
     */
    @Override
    public boolean deleteAllFromRealm() {
        realm.checkIfValid();
        if (size() > 0) {
            if (nativePtr == 0) {
                TableOrView table = getTableOrView();
                table.clear();
            } else {
                nativeClear(nativePtr);
            }
            return true;
        } else {
            return false;
        }
    }

