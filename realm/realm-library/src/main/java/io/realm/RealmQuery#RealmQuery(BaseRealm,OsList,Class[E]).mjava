    private RealmQuery(BaseRealm realm, OsList osList, Class<E> clazz) {
        this.realm = realm;
        this.clazz = clazz;
        this.schema = realm.getSchema().getSchemaForClass(clazz);
        this.table = schema.getTable();
        this.osList = osList;
        this.query = osList.getQuery();
    }

