    // Called by JNI
    @SuppressWarnings("unused")
    private long[] getObservedRowPtrs(Observer[] observers) {
        long[] ptrs = new long[observers.length];
        for (int i = 0; i < observers.length; i++) {
            ptrs[i]  = observers[i].row.getNativePtr();
        }
        return ptrs;
    }

