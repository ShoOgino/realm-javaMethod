    // Called by JNI
    @SuppressWarnings("unused")
    private long[] getObservedRowPtrs(RowObserverPair[] observerPairs) {
        long[] ptrs = new long[observerPairs.length];
        for (int i = 0; i < observerPairs.length; i++) {
            ptrs[i]  = observerPairs[i].row.getNativePtr();
        }
        return ptrs;
    }

