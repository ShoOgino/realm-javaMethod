        /**
         * {@inheritDoc}
         */
        @Override
        @Nullable
        public T next() {
            checkValid();
            pos++;
            if (pos >= iteratorCollection.size()) {
                throw new NoSuchElementException("Cannot access index " + pos + " when size is " + iteratorCollection.size() +
                        ". Remember to check hasNext() before using next().");
            }
            return get(pos);
        }

