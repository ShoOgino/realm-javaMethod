    // Called by JNI
    @KeepMember
    @SuppressWarnings("unused")
    private void notifyChangeListeners() {
        for (CollectionObserverPair pair: observerPairs) {
            Object object = pair.observerRef.get();
            if (object != null) {
                pair.onChange();
            } else {
                observerPairs.remove(pair);
            }
        }
    }

